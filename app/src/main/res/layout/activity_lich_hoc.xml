<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LichHoc.LichHocActivity">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/LichHoc_toolbar"/>
<CalendarView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@id/LichHoc_toolbar"
    android:id="@+id/LichHoc_callendar"
    />
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Events"
    android:id="@+id/tvEvenDay"
    android:textSize="20sp"
    android:layout_below="@id/LichHoc_callendar"
    android:layout_margin="5dp"
    />
</RelativeLayout>